<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fulladdlayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <ImageView
        android:id="@+id/fulladdimageView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        android:layout_marginBottom="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginTop="1dp"
        android:scaleType="fitXY" />

    <ImageView
        android:id="@+id/imageskip"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/skip" />

    <WebView
        android:id="@+id/fulladsweb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true" />

</RelativeLayout><!-- From: file:/F:/FakeCall/smadslib_New_v3/res/layout/fullscreenadd.xml --><!-- From: file:/F:/FakeCall/smadslib_New_v3/build/intermediates/bundles/debug/res/layout/fullscreenadd.xml -->